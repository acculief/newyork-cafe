<% @products_japan.each do |product| %>
<div class="thumbnail">
  <div class="video">
    <% iframe = content_tag(
        :iframe,
        '', # empty body
        src: "#{product.products_url.gsub("https://www.youtube.com/watch?v=", "https://www.youtube.com/embed/")}",
        frameborder: 0,
        allowfullscreen: true
    ) %>
    <%= content_tag(:div, iframe, class: 'youtube-container') %>
    <div class="Item">
      <p><%= product.title %></p>
      <div class="Item-Data">
        <p>再生回数 コメント数 お気に入り数</p>
      </div>
    </div>
  </div>
</div>
<% end %>
